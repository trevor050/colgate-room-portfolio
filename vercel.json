{
  "redirects": [
    {
      "source": "/api/:path*/",
      "destination": "/api/:path*",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/api/admin/:path*",
      "destination": "/api/admin?route=:path*"
    },
    {
      "source": "/_i/:path*",
      "destination": "https://us.i.posthog.com/:path*"
    }
  ]
}
